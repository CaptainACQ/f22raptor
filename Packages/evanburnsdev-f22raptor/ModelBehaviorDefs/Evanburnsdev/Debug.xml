	<Template Name="EVANBURNSDEV_ELECTRICAL_Switch_Bus_Toggle">
		<Component ID="#ID#" Node="#ID#">
			<DefaultTemplateParameters>
				<BUS_SRC>11</BUS_SRC>
				<BUS_DEST>10</BUS_DEST>
			</DefaultTemplateParameters>
			<UseTemplate Name="ASOBO_GT_Switch_Code">
				<ANIM_NAME>#ID#</ANIM_NAME>
				<NODE_ID>#ID#</NODE_ID>
				<PART_ID>#ID#</PART_ID>
				<LEFT_SINGLE_CODE>
					#BUS_SRC# #BUS_DEST# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE)
				</LEFT_SINGLE_CODE>
				<ANIM_CODE>
					#BUS_SRC# (&gt;A:BUS LOOKUP INDEX, Number)
					(A:BUS CONNECTION ON:#BUS_DEST#, Bool) 100 *
				</ANIM_CODE>
			</UseTemplate>
		</Component>
	</Template>
		
		
		
		
	<Template Name="EVANBURNSDEV_Softkey_Template">
		<DefaultTemplateParameters>
			<PREFIX>#DISPLAY#</PREFIX>
			<DISPLAY_POS>L</DISPLAY_POS>
			<KEY_PREFIX>#DISPLAY#_#ID#</KEY_PREFIX>
			<Condition>
				<Test>
					<Lower>
						<Value>KEY_ID</Value>
						<Number>10</Number>
					</Lower>
				</Test>
				<True>
					<UID>00#KEY_ID#</UID>
				</True>
				<False>
					<UID>0#KEY_ID#</UID>
				</False>
			</Condition>
		</DefaultTemplateParameters>
		<UseTemplate Name="EVANBURNSDEV_Softkey_SubTemplate"></UseTemplate>
	</Template>
