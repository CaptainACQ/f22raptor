	<Template Name="EVANBURNSDEV_DEBUG_ELEVATOR_DEFL">
		<UseTemplate Name="ASOBO_GT_Update">
			<FREQUENCY>5</FREQUENCY>
			<UPDATE_CODE>
				(A:ELEVATOR DEFLECTION, radians) 180 * 
				3.14159 / (&gt;L:XMLVAR_EBD_ELEVATOR_DEFL)
			</UPDATE_CODE>
		</UseTemplate>
		<UseTemplate Name="ASOBO_GT_Update">
			<FREQUENCY>1</FREQUENCY>
